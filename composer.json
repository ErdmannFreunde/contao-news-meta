{
    "name": "erdmannfreunde/contao-news-meta",
    "type": "contao-bundle",
    "description": "Contao-News-Meta",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Dennis Erdmann",
            "homepage": "https://www.erdmann-freunde.de"
        },
        {
            "name": "Frank Mueller",
            "homepage": "https://www.linking-you.de"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
